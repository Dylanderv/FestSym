<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FestiViteBundle\Entity\Offre" table="Offre">
    <id name="idoffre" type="integer" column="IdOffre">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="Type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="decimal" column="Prix" precision="9" scale="0" nullable="false"/>
    <field name="description" type="string" column="Description" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" type="string" column="Image" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idprofessionnel" type="integer" column="IdProfessionnel" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
