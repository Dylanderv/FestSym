<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FestiViteBundle\Entity\Soiree" table="Soiree">
    <id name="idsoiree" type="integer" column="IdSoiree">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="Nom" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="Adresse" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="decimal" column="Prix" precision="9" scale="0" nullable="true"/>
    <field name="type" type="string" column="Type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="Description" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreation" type="date" column="Date_Creation" nullable="false"/>
    <field name="dateSoiree" type="date" column="Date_Soiree" nullable="false"/>
    <field name="prixPers" type="decimal" column="Prix_Pers" precision="9" scale="0" nullable="true"/>
    <field name="adresseMail" type="string" column="Adresse_mail" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
