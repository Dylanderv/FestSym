<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FestiViteBundle\Entity\UtilisateurProfessionnel" table="Utilisateur_professionnel">
    <id name="idprofessionnel" type="decimal" column="IdProfessionnel">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="motDePasse" type="string" column="Mot_de_passe" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="Adresse" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numeroDeTelephone" type="string" column="Numero_de_telephone" length="14" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomSociete" type="string" column="Nom_Societe" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreation" type="date" column="Date_Creation" nullable="false"/>
    <field name="adresseMail" type="string" column="Adresse_mail" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
